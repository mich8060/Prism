{
    "manifest_version": 2,

    "name": "Pluralsight Prototype",
    "version": "1.0",
    "description": "An extension for Pluralsight.com to test with customers new concepts and designs.",

    "icons": {
        "16": "img/get_started16.png",
        "32": "img/get_started32.png",
        "48": "img/get_started48.png",
        "128": "img/get_started128.png"
    },

    "content_scripts": [{
        "css": ["styles.css"],
        "js": ["jquery-3.3.1.min.js","content.js"],
        "matches": ["https://app.pluralsight.com/*"]
    }]
}
